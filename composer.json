{
    "name": "sonata-project/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle",
    "keywords": ["blog", "sonata", "news", "post"],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "symfony/symfony": "~2.3|~3.0",
        "sonata-project/core-bundle": "^3.0",
        "sonata-project/admin-bundle": "^3.0",
        "sonata-project/user-bundle": "3.x-dev",
        "sonata-project/media-bundle": "2.4.x-dev",
        "sonata-project/classification-bundle": "3.x-dev",
        "sonata-project/formatter-bundle": "2.4.x-dev",
        "sonata-project/easy-extends-bundle": "~2.1",
        "sonata-project/intl-bundle": "~2.1",
        "sonata-project/datagrid-bundle": "~2.2"
    },
    "require-dev": {
        "doctrine/orm": "*",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/notification-bundle": "3.x-dev",
        "sonata-project/seo-bundle": "3.x-dev",
        "friendsofsymfony/rest-bundle": "~1.1",
        "nelmio/api-doc-bundle": "~2.4",
        "jms/serializer-bundle": "~0.11|~1.0"
    },
    "conflict": {
        "jms/serializer": "<0.13"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/notification-bundle": "3.x-dev"
    },
    "autoload": {
        "psr-4": { "Sonata\\NewsBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
