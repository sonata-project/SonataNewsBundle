{
    "name": "sonata-project/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle",
    "keywords": [
        "blog",
        "sonata",
        "news",
        "post"
    ],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/persistence": "^1.3 || ^2.0",
        "sonata-project/classification-bundle": "^4.0@dev",
        "sonata-project/doctrine-extensions": "^1.13",
        "sonata-project/form-extensions": "^1.8",
        "sonata-project/formatter-bundle": "^4.6",
        "sonata-project/intl-bundle": "^2.10",
        "sonata-project/media-bundle": "^4.0@dev",
        "sonata-project/twig-extensions": "^1.5",
        "symfony/config": "^4.4",
        "symfony/console": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/event-dispatcher": "^4.4",
        "symfony/event-dispatcher-contracts": "^1.1 || ^2.0",
        "symfony/form": "^4.4",
        "symfony/framework-bundle": "^4.4.24",
        "symfony/http-foundation": "^4.4",
        "symfony/http-kernel": "^4.4",
        "symfony/mailer": "^4.4 || ^5.1",
        "symfony/mime": "^4.4.10 || ^5.1",
        "symfony/options-resolver": "^4.4",
        "symfony/routing": "^4.4",
        "symfony/security-core": "^4.4.23",
        "symfony/templating": "^4.4 || ^5.1",
        "symfony/translation-contracts": "^1.1 || ^2.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12.1"
    },
    "conflict": {
        "doctrine/dbal": "<2.6.0",
        "doctrine/mongodb-odm": "<2.2",
        "sonata-project/admin-bundle": "<4.0",
        "sonata-project/block-bundle": "<3.18",
        "sonata-project/core-bundle": "<3.20",
        "sonata-project/doctrine-orm-admin-bundle": "<4.0",
        "sonata-project/notification-bundle": "<4.0",
        "sonata-project/seo-bundle": "<3.0"
    },
    "require-dev": {
        "doctrine/mongodb-odm": "^2.2",
        "doctrine/orm": "^2.4",
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-phpunit": "^0.12.22",
        "psalm/plugin-phpunit": "^0.16.1",
        "sonata-project/admin-bundle": "^4.0",
        "sonata-project/block-bundle": "^4.7",
        "sonata-project/doctrine-mongodb-admin-bundle": "^4.0",
        "sonata-project/doctrine-orm-admin-bundle": "^4.0",
        "sonata-project/notification-bundle": "^4.0@dev",
        "sonata-project/seo-bundle": "^3.0@dev",
        "symfony/browser-kit": "^4.4",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/swiftmailer-bundle": "^3.4",
        "vimeo/psalm": "^4.10"
    },
    "suggest": {
        "sonata-project/block-bundle": "For rendering block lists.",
        "sonata-project/doctrine-orm-admin-bundle": "For integrate Doctrine ORM into the SonataAdminBundle.",
        "sonata-project/notification-bundle": "If you want to send comment notification asynchronously.",
        "twig/extra-bundle": "Auto configures the Twig Intl extension."
    },
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Sonata\\NewsBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sonata\\NewsBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/simple-phpunit install"
        ],
        "post-update-cmd": [
            "[ $COMPOSER_DEV_MODE -eq 0 ] || vendor/bin/simple-phpunit install"
        ]
    },
    "abandoned": true
}
