{
    "name": "rmzamora/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle",
    "keywords": ["blog", "sonata", "news", "post", "rmzamora forked"],
    "homepage": "http://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "http://sonata-project.org"
        },
        {
            "name": "mell zamora",
            "email": "me@rmzamora.com",
            "homepage": "http://rmzamora.com"
        },         
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "symfony/symfony": "2.1.*",
        "sonata-project/admin-bundle": "dev-master",
        "sonata-project/user-bundle": "dev-master",
        "rmzamora/media-bundle": "dev-master",
        "rmzamora/formatter-bundle": "dev-master",
        "sonata-project/easy-extends-bundle": "dev-master",
        "sonata-project/intl-bundle": "dev-master",
        "knplabs/knp-markdown-bundle": "dev-master"
    },
    "require-dev": {
        "doctrine/orm": "*"
    },
    "suggest": {
        "sonata-project/doctrine-orm-admin-bundle": "dev-master"
    },
    "autoload": {
        "psr-0": { "Sonata\\NewsBundle": "" }
    },
    "target-dir": "Sonata/NewsBundle"
}