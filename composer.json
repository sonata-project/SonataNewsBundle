{
    "name": "sonata-project/news-bundle",
    "type": "symfony-bundle",
    "description": "Symfony SonataNewsBundle",
    "keywords": ["blog", "sonata", "news", "post"],
    "homepage": "https://sonata-project.org/bundles/news",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Rabaix",
            "email": "thomas.rabaix@sonata-project.org",
            "homepage": "https://sonata-project.org"
        },
        {
            "name": "Sonata Community",
            "homepage": "https://github.com/sonata-project/SonataNewsBundle/contributors"
        }
    ],
    "require": {
        "php": "^5.3 || ^7.0",
        "symfony/console": "^2.3",
        "symfony/form": "^2.3",
        "symfony/framework-bundle": "^2.3",
        "symfony/templating": "^2.3",
        "sonata-project/admin-bundle": "^3.4",
        "sonata-project/user-bundle": "^3.0",
        "sonata-project/media-bundle": "^3.0",
        "sonata-project/classification-bundle": "^3.0",
        "sonata-project/formatter-bundle": "^3.0",
        "sonata-project/easy-extends-bundle": "^2.2",
        "sonata-project/intl-bundle": "^2.2.4",
        "sonata-project/datagrid-bundle": "^2.2.1",
        "sonata-project/core-bundle": "^3.0",
        "swiftmailer/swiftmailer": "^4.3 || ^5.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.4",
        "sonata-project/block-bundle": "^3.2",
        "sonata-project/doctrine-orm-admin-bundle": "^3.0",
        "sonata-project/notification-bundle": "^3.0",
        "sonata-project/seo-bundle": "^2.0",
        "friendsofsymfony/rest-bundle": "^1.1",
        "nelmio/api-doc-bundle": "^2.4",
        "jms/serializer-bundle": "^0.13 || ^1.0",
        "sllh/php-cs-fixer-styleci-bridge": "^2.0"
    },
    "conflict": {
        "jms/serializer": "<0.13",
        "sonata-project/block-bundle": "<3.2",
        "sonata-project/doctrine-orm-admin-bundle": "<3.0 || >=4.0",
        "sonata-project/notification-bundle": "<3.0 || >=4.0",
        "sonata-project/seo-bundle": "<2.0 || >=3.0"
    },
    "suggest": {
        "sonata-project/block-bundle": "For rendering block lists.",
        "sonata-project/doctrine-orm-admin-bundle": "^2.2",
        "sonata-project/notification-bundle": "^2.2"
    },
    "autoload": {
        "psr-4": { "Sonata\\NewsBundle\\": "" },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": { "Sonata\\NewsBundle\\Tests\\": "Tests/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.x-dev"
        }
    }
}
